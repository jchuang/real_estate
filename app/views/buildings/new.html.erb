<p class="notice"><%= notice %></p>

<h1>New Building</h1>

<%= simple_form_for @building do |f| %>

  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :state, collection: Building.state_abbreviations %>
  <%= f.input :zip_code, label: 'Zip Code' %>
  <%= f.input :description %>
  <%= f.button :submit %>

<% end %>
